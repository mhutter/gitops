{
  "extends": ["config:base"],
  "argocd": {
    "fileMatch": [
      "^root/.+\\.ya?ml$",
      "^install\\.yml$"
    ]
  },
  "kubernetes": {
    "fileMatch": [
      "^()/.+\\.ya?ml$"
    ]
  },
  "kustomize": {
    "fileMatch": [
      "(^|/)kustomization\\.ya?ml$"
    ]
  },
  "regexManagers": [{
    "fileMatch": ["^system-upgrade/plan-.+\\.ya?ml$"],
    "matchStrings": ["version: (?<currentValue>)\n"],
    "datasourceTemplate": "github-releases",
    "depNameTemplate": "kubernetes/kubernetes"
  }]
}
